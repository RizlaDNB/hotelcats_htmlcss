# REDIRECT FAVICON.ICO
<ifmodule mod_rewrite.c>
RewriteCond %{REQUEST_URI} !^/favicon\.ico [NC]
RewriteCond %{REQUEST_URI} favicon\.ico    [NC]
RewriteRule (.*) https://rizladnb.github.io/hotelcats_htmlcss/img/favicon.ico [R=301,L]
</ifmodule>

# REDIRECT AJAX-LOADER
<ifmodule mod_rewrite.c>
RewriteCond %{REQUEST_URI} !^/images/ajax\-loader\.gif [NC]
RewriteCond %{REQUEST_URI} ajax\-loader\.gif           [NC]
RewriteRule (.*) https://rizladnb.github.io/hotelcats_htmlcss/img/ajax-loader.gif [R=301,L]
</ifmodule>